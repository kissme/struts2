<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lxitedu.model.bean.User" >
	<id name="userID">
		<generator class="increment"></generator>
	</id>
	<property name="userName"></property>
	<property name="userPassword"></property>
	<property name="sex"></property>
	<property name="hobby"></property>
	<property name="nation"></property>
	<property name="birthday"></property>
	</class>
	
	<class name="com.lxitedu.model.bean.Hobby">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="name"></property>
	</class>
	
	<class name="com.lxitedu.model.bean.Nation">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="name"></property>
	</class>
</hibernate-mapping>
